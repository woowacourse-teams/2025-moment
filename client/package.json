{
  "name": "client",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm exec webpack serve --config webpack.dev.js",
    "dev:analyze": "ANALYZE_BUNDLE=true pnpm exec webpack serve --config webpack.dev.js",
    "build": "pnpm exec webpack --config webpack.prod.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "pnpm run lint && pnpm run format:check",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:silent": "jest --silent",
    "test:debug": "jest --no-cache --verbose",
    "converter:csv": "node src/converter/csv/csvToTs.js"
  },
  "license": "MIT",
  "sideEffects": [
    "*.css",
    "@emotion/**/*",
    "@sentry/**/*",
    "react-ga4/**/*"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@sentry/react": "^9.43.0",
    "@tanstack/react-query": "^5.83.0",
    "axios": "^1.10.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-ga4": "^2.1.0",
    "react-router": "^7.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@eslint-react/eslint-plugin": "^1.52.3",
    "@storybook/addon-docs": "9.0.16",
    "@storybook/addon-webpack5-compiler-swc": "3.0.0",
    "@storybook/react-webpack5": "^9.0.16",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "babel-jest": "^30.0.4",
    "babel-loader": "^10.0.0",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-storybook": "9.0.16",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "mini-css-extract-plugin": "^2.9.4",
    "msw": "^2.10.4",
    "plugin": "^0.3.3",
    "prettier": "^3.6.2",
    "storybook": "^9.0.17",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.37.0",
    "webpack": "^5.100.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^6.0.1"
  },
